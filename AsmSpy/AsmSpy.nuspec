    <copyright>Copyright © Suteki Ltd</copyright>
    <licenseUrl>https://github.com/mikehadlow/AsmSpy/blob/master/licence.txt</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <releaseNotes>Publishing to Chocolatey</releaseNotes>
  </metadata>
  <files>
    <file src="bin\Debug\AsmSpy.exe" target="tools" />
    <file src="bin\Debug\Microsoft.Extensions.CommandLineUtils.dll" target="tools" />
    <file src="License.txt" target="tools" />
  </files>
</package>
